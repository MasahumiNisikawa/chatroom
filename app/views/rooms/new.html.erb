<h1>ルーム新規作成</h1>
<%= form_with model: @room, local: true do |f| %>
    <div class="form-group">
      <label>名前</label>
      <%= f.text_field :room_name, class: "form-control" %>
    </div>
 <%= link_to "戻る", rooms_path, class: "btn btn-secondary" %>   
 <%= f.submit "登録", class: "btn btn-primary"%>
<% end %>